<%- messages() %>

<main id="account-management" class="management">
    <h2>Welcome <%= accountData.account_firstname %></h2>

    <div class="management-actions">
      <a href="/account/update/<%= accountData.account_id %>" class="btn">Update Account Information</a>
    </div>
  
    <% if (accountData.account_type === "Employee" || accountData.account_type === "Admin") { %>
      <section>
        <h2>Inventory Management</h2>
        <p><a href="/inv" class="btn">Go to Inventory Management</a></p>
      </section>
    <% } %>
</main>
  