<h1>Welcome to CSE Motors!</h1>

<%- messages() %>

<form id="loginForm" action="/account/login" method="post">

    <!-- EMAIL  -->
    <label for="account_email">E-mail: </label>
    <input type="email" id="account_email" name="account_email" required title="Enter your email."
    autocomplete="email" placeholder="example@email.com">

    <!-- PASSWORD  -->
    <label for="account_password">Password: </label>
    <input type="password" id="account_password" name="account_password" required title="Enter your password.">



    <button id="loginSubmit" type="submit">Login</button>

</form>

<p class="register">Don't have an account? <a href="/account/register">Register here</a></p>